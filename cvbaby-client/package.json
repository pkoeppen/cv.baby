    "lodash": "^4.17.11",
    "nuxt": "^2.4.0",
    "nuxt-i18n": "^5.12.4",
    "vue-mq": "^1.0.1",
    "vuetify": "^1.5.5",
    "vuetify-loader": "^1.2.1"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^0.0.1",
