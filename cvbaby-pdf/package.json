{
  "name": "cvbaby-pdf",
  "private": true,
  "scripts": {
    "deploy:dev": "npm run build:prod && serverless deploy --stage dev",
    "deploy:prod": "npm run build:prod && serverless deploy --stage prod",
    "build:dev": "make dev",
    "build:prod": "make prod",
    "dev": "serverless offline start",
    "docker": "node docker.js",
    "install-chrome": "git clone https://github.com/alixaxel/chrome-aws-lambda",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "remove:dev": "serverless remove --stage dev",
    "remove:prod": "serverless remove --stage prod"
  },
  "devDependencies": {
    "docker-lambda": "^0.15.3",
    "serverless-offline": "^4.10.6"
  }
}
