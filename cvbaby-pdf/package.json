{
  "name": "cvbaby-pdf",
  "private": true,
  "scripts": {
    "deploy:dev": "npm run build:package && serverless deploy --stage dev",
    "deploy:prod": "npm run build:package && serverless deploy --stage prod",
    "build:local": "make local",
    "build:package": "make package",
    "local": "serverless offline start --stage local",
    "docker": "node docker.js",
    "install-chrome": "git clone https://github.com/alixaxel/chrome-aws-lambda",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "remove:dev": "serverless remove --stage dev"
  },
  "devDependencies": {
    "docker-lambda": "^0.15.3",
    "serverless-offline": "^4.10.6"
  }
}
